        "gpii": {
            "command": "gpii-app.exe",
            "ipc": "gpii",
            "autoRestart": true
        },
        "rfid-listener": {
            "command": "../listeners/GPII_RFIDListener.exe",
            "autoRestart": true
        },
        "usb-listener": {
