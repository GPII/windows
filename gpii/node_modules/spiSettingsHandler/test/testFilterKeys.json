{
    "options": {
        "getAction": "SPI_GETFILTERKEYS",
        "setAction": "SPI_SETFILTERKEYS",
        "uiParam": "struct_size",
        "pvParam": {
            "type": "struct",
            "name": "FILTERKEYS",
            "template": {
                "cbSize": "uint32",
                "dwFlags": "uint32",
                "iWaitMSec": "uint32",
                "iDelayMSec": "uint32",
                "iRepeatMSec": "uint32",
                "iBounceMSec": "uint32"
            }
        }
    },
    
    "settings": {
        "FilterKeysAvailable": {
            "value": true,
            "path": "pvParam.dwFlags.FKF_AVAILABLE"
        },
        
        "ConfirmHotkey": {
            "value": true,
            "path": "pvParam.dwFlags.FKF_CONFIRMHOTKEY"
        },
        
        "Wait": {
            "value": 1000,
            "path": "pvParam.iWaitMSec"
        },
        
        "Delay": {
            "value": 1000,
            "path": "pvParam.iDelayMSec"
        }
    }
}
