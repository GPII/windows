{
    "options": {
        "getAction": "SPI_GETMOUSEKEYS",
        "setAction": "SPI_SETMOUSEKEYS",
        "uiParam": "struct_size",
        "pvParam": {
            "type": "struct",
            "name": "MOUSEKEYS",
            "template": {
                "cbSize": "uint32",
                "dwFlags": "uint32",
                "iMaxSpeed": "uint32",
                "iTimeToMaxSpeed": "uint32",
                "iCtrlSpeed": "uint32",
                "dwReserved1": "uint32",
                "dwReserved2": "uint32"
            }
        }
    },

    "settings": {
        "FilterKeysAvailable": {
            "value": true,
            "path": "pvParam.dwFlags.MKF_AVAILABLE"
        },

        "MouseKeysOn": {
            "value": false,
            "path": "pvParam.dwFlags.MKF_MOUSEKEYSON"
        },

        "MaxSpeed": {
            "value": 100,
            "path": "pvParam.iMaxSpeed"
        },

        "Acceleration": {
            "value": 1000,
            "path": "pvParam.iTimeToMaxSpeed"
        },

        "CtrlSpeed": {
            "value": 2,
            "path": "pvParam.iCtrlSpeed"
        }
    }
}
